doi = {DOI 10.1016/j.aquaculture.2010.10.026},
issn = {0044-8486},
journal = {Aquaculture},
keywords = {solea senegalensis sparus aurata metamorphosis aro},
language = {English},
pages = {200--205},
title = {{Can dietary aromatic amino acid supplementation be beneficial during fish metamorphosis?}},
