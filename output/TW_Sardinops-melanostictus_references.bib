doi = {10.1890/04-1620},
file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Heibo, Magnhagen, V{\o}llestad{\_}2005{\_}Latitudinal Variation in Life History Traits in Eurasian Perch.pdf:pdf},
