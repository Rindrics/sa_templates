issn = {0257-7615},
journal = {South African Journal of Marine Science-Suid-Afrikaanse Tydskrif Vir Seewetenskap},
language = {English},
pages = {275--290},
title = {{The effect of sea temperature and food availability on the spawning success of Cape anchovy Engraulis capensis in the southern Benguela}},
volume = {19},
year = {1998}
