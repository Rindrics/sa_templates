doi = {10.1023/A:1025352026974},
file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Vila-Gispert, Moreno-Amich, Garc{\'{i}}a-Berthou{\_}2002{\_}Gradients of life-history variation An intercontinental comparison of fishes.pdf:pdf},
