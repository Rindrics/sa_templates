Cited References Count:311},
author = {Peck, M A and Reglero, P and Takahashi, M and Catalan, I A},
doi = {DOI 10.1016/j.pocean.2013.05.012},
file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Peck et al.{\_}2013{\_}Life cycle ecophysiology of small pelagic fish and climate-driven changes in populations.pdf:pdf},
