issn = {0022-1112},
journal = {Journal of Fish Biology},
keywords = {developmental rates larval growth metamorphosis mo},
pages = {825--837},
title = {{Transition period from larva to juvenile in anchoveta Engraulis ringens. Length or age related?}},
