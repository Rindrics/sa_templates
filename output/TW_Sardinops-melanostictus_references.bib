issn = {0022-3670},
journal = {Journal of Physical Oceanography},
keywords = {Circulation/ Dynamics,Currents,Geographic location/entity,Meridional overturning circulation,North Pacific Ocean,Observational techniques and algorithms,Profilers,Satellite observations,Ship observations,oceanic},
number = {6},
pages = {1636--1653},
title = {{Currents Associated with the Quasi-Stationary Jet Separated from the Kuroshio Extension}},
url = {http://journals.ametsoc.org/doi/abs/10.1175/JPO-D-12-0192.1},
