author = {Payne, M R and Ross, S D and Clausen, L W and Munk, P and Mosegaard, H and Nash, R D M},
doi = {Doi 10.3354/Meps10392},
issn = {0171-8630},
journal = {Marine Ecology Progress Series},
keywords = {north sea herring otolith microstructure hydrograp},
language = {English},
pages = {197--211},
title = {{Recruitment decline in North Sea herring is accompanied by reduced larval growth rates}},
volume = {489},
year = {2013}
}
@article{Kawahata2002,
