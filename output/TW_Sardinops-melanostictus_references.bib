doi = {10.2989/025776199784126088},
file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Barange, Hampton, Roel{\_}1999{\_}Trends in the abundance and distribution of anchovy and sardine on the South African continental shelf in th.pdf:pdf},
isbn = {0257-7615},
issn = {0257-7615},
journal = {South African Journal of Marine Science},
