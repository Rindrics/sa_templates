issn = {0029-8549},
journal = {Oecologia},
keywords = {hydrodynamic stress somatic growth otolith daily i},
language = {English},
pages = {567--576},
title = {{Three different patterns of how low-intensity waves can affect the energy budget of littoral fish: a mesocosm study}},
