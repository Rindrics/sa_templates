file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Yasue, Takasuka{\_}2009{\_}Seasonal variability in growth of larval Japanese anchovy Engraulis japonicus driven by fluctuations in sea tempera.pdf:pdf},
isbn = {1095-8649 (Electronic)$\backslash$n0022-1112 (Linking)},
issn = {1095-8649},
journal = {Journal of fish biology},
keywords = {Animals,Body Size,Body Size: physiology,Fishes,Fishes: growth {\&} development,Japan,Oceans and Seas,Otolithic Membrane,Otolithic Membrane: anatomy {\&} histology,Population Density,Seasons,Temperature,Zooplankton,Zooplankton: physiology},
number = {10},
pages = {2250--68},
pmid = {20735551},
title = {{Seasonal variability in growth of larval Japanese anchovy Engraulis japonicus driven by fluctuations in sea temperature in the Kii Channel, Japan.}},
