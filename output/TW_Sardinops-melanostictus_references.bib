annote = {124VZ
Times Cited:46
Cited References Count:63},
author = {van der Lingen, C D and Hutchings, L and Field, J G},
issn = {1814-232X},
journal = {African Journal of Marine Science},
keywords = {anchovy sardine southern benguela species alternat},
language = {English},
pages = {465--477},
title = {{Comparative trophodynamics of anchovy Engraulis encrasicolus and sardine Sardinops sagax in the southern Benguela: are species alternations between small pelagic fish trophodynamically mediated?}},
