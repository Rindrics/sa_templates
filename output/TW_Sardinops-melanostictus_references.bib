issn = {09247963},
journal = {Journal of Marine Systems},
keywords = {Ecopath model,Ecosystem Approach to Fisheries,comparative approach,fishing impacts,mass-balance model,northwestern Mediterranean Sea},
title = {{Ecosystem structure and fishing impacts in the northwestern Mediterranean Sea using a food web model within a comparative approach}},
