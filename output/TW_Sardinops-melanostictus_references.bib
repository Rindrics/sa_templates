doi = {DOI 10.1093/plankt/fbm072},
issn = {0142-7873},
journal = {Journal of Plankton Research},
keywords = {lanternfish larvae leeuwin current coastal waters},
language = {English},
pages = {967--983},
title = {{Seasonal variation in horizontal and vertical structure of larval fish assemblages off south-western Australia, with implications for larval transport}},
