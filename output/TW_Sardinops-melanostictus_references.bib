issn = {0022-1112},
journal = {Journal of Fish Biology},
keywords = {cloning clupeiformes gonadotroph gonadotropin immu},
language = {English},
pages = {372--387},
title = {{Molecular cloning and localization of the luteinizing hormone beta subunit and glycoprotein hormone alpha subunit from Japanese anchovy Engraulis japonicus}},
