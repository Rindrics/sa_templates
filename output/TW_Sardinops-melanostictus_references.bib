journal = {CalCOFI Rep.},
pages = {105--111},
title = {{Sardine and Anchovy Spawning As Related To Temperature and Upwelling in the California Current System}},
url = {http://cisweb4.ucsd.edu/newhome/publications/CalCOFI{\_}Reports/v32/pdfs/Vol{\_}32{\_}Lluch-Belda{\_}etal.pdf},
