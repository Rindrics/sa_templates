issn = {1054-6006},
journal = {Fisheries Oceanography},
keywords = {Engraulis,Marine,Q1 01603 Fishery statistics and sampling,Q5 01521 Mechanical and,Sardinops,World Oceans,abundance,climatic changes,commercial species,dynamics,fishery statistics,fishery surveys,natural changes,ocean currents,population},
number = {4},
pages = {339--347},
pmid = {3542708},
title = {{Sardine and anchovy regime fluctuations of abundance in four regions of the world oceans: A workshop report}},
volume = {1},
year = {1992}
}
@article{Ospina-Alvarez2013,
abstract = {To date, there are numerous transport simulation studies demonstrating the relevance of the hydrodynamics for the advection, dispersion and recruitment of early stages of marine organisms. However, the lack of data has conditioned the use of realistic locations for the model setup and configuration in transport studies. This work (I) demonstrates the key role played by the use of the realistic initial position of the eggs of small pelagic fishes in the analysis of late-larval recruitment in coastal nursery areas and (II) provides a general solution for deriving future egg positions and abundances from adult biomass obtained from acoustic surveys and available fecundity data. Using European anchovy in the NW Mediterranean as a case study, we first analyzed the impact of the initial location, timing, egg buoyancy and diel vertical migration of larvae on the potential late-larval recruitment to coastal areas. The results suggested that prior knowledge of the initial spawning grounds may substantially affect the estimates of potential recruitment. We then integrated biological and acoustics-derived data (the biomass and size structure, sex ratio, a weight-batch fecundity model, mean weight, number of fish and mean spawning) to build a predictive model for interannual egg production. This model was satisfactorily contrasted with field data for two years obtained with the Daily Egg Production Method (DEPM). We discuss our results in the context of the fluctuations of European anchovy egg abundance from 2003 through 2010 in the NW Mediterranean and in terms of the potential applicability of the acoustics-based spatial predictive egg production model.},
author = {Ospina-{\'{A}}lvarez, Andr{\'{e}}s and Bernal, Miguel and Catal{\'{a}}n, Ignacio Alberto and Roos, David and Bigot, Jean Louis and Palomera, Isabel},
doi = {10.1371/journal.pone.0073687},
file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Ospina-{\'{A}}lvarez et al.{\_}2013{\_}Modeling Fish Egg Production and Spatial Distribution from Acoustic Data A Step Forward into the Analysis of.pdf:pdf},
isbn = {1932-6203},
issn = {19326203},
journal = {PLoS ONE},
number = {9},
pages = {1--18},
pmid = {24066063},
title = {{Modeling Fish Egg Production and Spatial Distribution from Acoustic Data: A Step Forward into the Analysis of Recruitment}},
volume = {8},
year = {2013}
}
@article{Petitgas2012,
abstract = {The abundance and spatial occupation of European anchovy Engraulis encrasicolus have increased in the North Sea since the mid-1990s. We use a cross-disciplinary approach combining genetics, transport modelling, survey time series analyses and physical oceanographic modelling to investigate 3 hypotheses on the reasons for this change. Evidence from connectivity studies suggests that the population of North Sea anchovy is separate from that in the Bay of Biscay. The recruitment pulses observed in survey data fit a life cycle which includes spawning in early summer and larval development in late summer. This also supports the concept of population expansion originating from local remnant population(s). In terms of growth physiology, suitable thermal windows have expanded, making conditions more favourable for life cycle closure and population persistence/productivity. In addition to the increased frequency of warm summers, which favour larvae and juvenile growth, the decrease in the number of severe winters is also likely to improve overwinter survival. Overall, the evidence supports the hypothesis that the increase in anchovy abundance originated from the improved productivity of existing populations. This increase was associated with an expansion in thermal habitats and is probably not due to a northward shift in the distribution of southern conspecifics.},
author = {Petitgas, Pierre and Alheit, J{\"{u}}rgen and Peck, Myron a. and Raab, Kristina and Irigoien, Xabier and Huret, Martin and {Van Der Kooij}, Jeroen and Pohlmann, Thomas and Wagner, Carola and Zarraonaindia, Iratxe and Dickey-Collas, Mark},
doi = {10.3354/meps09451},
file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Petitgas et al.{\_}2012{\_}Anchovy population expansion in the North Sea.pdf:pdf},
issn = {01718630},
journal = {Marine Ecology Progress Series},
keywords = {Anchovy,Climate variability,North Sea,Regime shift,Small pelagic fish,Temperature},
pages = {1--13},
title = {{Anchovy population expansion in the North Sea}},
volume = {444},
year = {2012}
}
