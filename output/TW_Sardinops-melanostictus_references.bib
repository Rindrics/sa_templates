title = {{Detection of Contaminant and Climate Effects on Spawning Success of 3 Pelagic Fish Stocks Off Southern California - Northern Anchovy Engraulis-Mordax, Pacific Sardine Sardinops-Sagax, and Chub Mackerel Scomber-Japonicus}},
volume = {91},
