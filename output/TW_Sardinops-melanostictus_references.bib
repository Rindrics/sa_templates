number = {3},
pages = {310--324},
pmid = {12714014},
title = {{Effect of photoperiod and temperature on gonadal activity and plasma steroid levels in a reared strain of the mummichog (Fundulus heteroclitus) during different phases of its annual reproductive cycle}},
