journal = {African Journal of Marine Science},
keywords = {benguela current drift pattern early life history},
language = {English},
pages = {195--204},
title = {{Spawning strategies and transport of early stages of the two Cape hake species, Merluccius paradoxus and M. capensis, in the southern Benguela upwelling system}},
