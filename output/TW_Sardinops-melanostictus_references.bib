journal = {The Journal of experimental biology},
keywords = {atlantic herring,clupea harengus,dominated,drag,hydrodynamics,larvae altered both transverse,locomotion,q 10,swimming,tail speed and,temperature,viscosity},
number = {Pt 12},
pages = {1745--55},
pmid = {9319652},
title = {{What a drag it is getting cold: partitioning the physical and physiological effects of temperature on fish swimming}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9319652},
volume = {200},
year = {1997}
