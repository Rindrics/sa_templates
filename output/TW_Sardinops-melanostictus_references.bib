issn = {02784343},
journal = {Continental Shelf Research},
keywords = {Air-sea flux,Bay of Biscay,Climatic change,Heat budget,Interannual temperature variability,Ocean circulation model,biscay bay temperature oceanograph},
mendeley-tags = {biscay bay temperature oceanograph},
pages = {1070--1087},
title = {{Temperature variability in the Bay of Biscay during the past 40 years, from an in situ analysis and a 3D global simulation}},
