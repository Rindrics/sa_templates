author = {Castro, L R and Hernandez, E H},
issn = {0002-8487},
journal = {Transactions of the American Fisheries Society},
keywords = {bay anchovy growth mortality larvae mitchilli fish},
language = {English},
pages = {1107--1117},
title = {{Early life survival of the anchoveta Engraulis ringens off central Chile during the 1995 and 1996 winter spawning seasons}},
volume = {129},
