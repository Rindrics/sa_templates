doi = {Doi 10.1071/Mf04140},
issn = {1323-1650},
journal = {Marine and Freshwater Research},
keywords = {alizarin feeding experiment postlarval sprat size-},
language = {English},
pages = {539--547},
title = {{Short-term decoupling of otolith and somatic growth induced by food level changes in postlarval Baltic sprat, Sprattus sprattus}},
volume = {56},
