journal = {Fisheries Research},
keywords = {Anchoveta fishery,ITQ system,IVQ system,Overcapacity,Peru,Race for fish},
pages = {308--312},
title = {{Developments on fisheries management in Peru: The new individual vessel quota system for the anchoveta fishery}},
volume = {96},
year = {2009}
}
