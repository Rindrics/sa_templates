issn = {0722-4028},
journal = {Coral Reefs},
keywords = {coral reef herbivorous fishes macroalgae phase shi},
language = {English},
pages = {1005--1015},
title = {{Coral-macroalgal phase shifts or reef resilience: links with diversity and functional roles of herbivorous fishes on the Great Barrier Reef}},
