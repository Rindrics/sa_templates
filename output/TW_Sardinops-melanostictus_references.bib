file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Palomera{\_}1992{\_}Spawning of Anchovy Engraulis-Encrasicolus in the Northwestern Mediterranean Relative to Hydrographic Features in the Regi.pdf:pdf},
isbn = {0171-8630},
issn = {01718630},
journal = {Marine Ecology Progress Series},
number = {3},
pages = {215--223},
title = {{Spawning of anchovy Engraulis encrasicolus in the northwestern Mediterranean relative to hydrographic features in the region}},
volume = {79},
year = {1992}
