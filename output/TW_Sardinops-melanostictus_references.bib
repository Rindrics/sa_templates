journal = {The Journal of experimental biology},
keywords = {homing,olfactory imprinting,salmon,thyroid hormones},
pages = {83--91},
pmid = {9317381},
title = {{Homing in Pacific salmon: mechanisms and ecological basis}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9317381},
volume = {199},
year = {1996}
}
