file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Morimoto, Morimoto{\_}1998{\_}Relationship between batch fecundity and egg size in Japanese sardine Sardinops melanostictus in Tosa bay and of.pdf:pdf},
