pages = {1909--1922},
title = {{Effect of Age, Growth-Rate, and Ontogeny on the Otolith Size - Fish Sire Relationship in Bluefish, Pomatomus Saltatrix, and the Implications for Back-Calculation of Sire in Fish Early-Life History Stages}},
volume = {52},
