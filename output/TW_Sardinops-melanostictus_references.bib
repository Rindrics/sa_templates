doi = {DOI 10.1111/j.1095-8649.2009.02361.x},
issn = {0022-1112},
journal = {Journal of Fish Biology},
keywords = {clupeidae daily egg production fecundity reproduct},
language = {English},
pages = {1560--1576},
title = {{Spawning dynamics and biomass estimates of an anchovy Engraulis australis population in contrasting gulf and shelf environments}},
