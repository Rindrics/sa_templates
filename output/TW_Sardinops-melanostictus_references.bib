author = {Mah, J H and Hwang, H J},
doi = {DOI 10.1016/j.foodchem.2008.09.035},
issn = {0308-8146},
journal = {Food Chemistry},
keywords = {biogenic amines salted and fermented anchovy myeol},
language = {English},
pages = {168--173},
title = {{Effects of food additives on biogenic amine formation in Myeolchi-jeot, a salted and fermented anchovy (Engraulis japonicus)}},
volume = {114},
