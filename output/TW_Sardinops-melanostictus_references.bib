doi = {DOI 10.1007/s10750-010-0398-1},
issn = {0018-8158},
journal = {Hydrobiologia},
keywords = {common bream abramis brama daphnia echosounder lep},
language = {English},
pages = {253--265},
title = {{Sinusoidal swimming in fishes: the role of season, density of large zooplankton, fish length, time of the day, weather condition and solar radiation}},
url = {http://link.springer.com/content/pdf/10.1007{\%}2Fs10750-010-0398-1},
