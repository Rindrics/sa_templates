isbn = {1054-6006},
issn = {1054-6006},
journal = {Fisheries Oceanography},
keywords = {atlantic,climate variation,copepods,density gradient,mixed layer,model,nutrient,oyashio,phytoplankton,sea,spring bloom,subarctic pacific},
number = {April},
pages = {118--129},
title = {{Variability in timing and magnitude of spring bloom in the Oyashio region, the western subarctic Pacific off Hokkaido, Japan}},
volume = {6},
