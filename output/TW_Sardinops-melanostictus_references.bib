journal = {Molecular Biology and Evolution},
keywords = {mitochondrial DNA phylogeography european anchovy},
language = {English},
pages = {178--190},
title = {{Mitochondrial DNA phylogeny and the reconstruction of the population history of a species: The case of the European anchovy (Engraulis encrasicolus)}},
