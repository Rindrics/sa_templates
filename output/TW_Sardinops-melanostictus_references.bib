file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Huggett et al.{\_}2003{\_}Modelling the transport success of anchovy Engraulis encrasicolus eggs and larvae in the southern Benguela the effec.pdf:pdf},
issn = {0171-8630},
journal = {Marine Ecology-Progress Series},
