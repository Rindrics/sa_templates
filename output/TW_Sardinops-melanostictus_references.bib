file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Himeoka, Kaneko{\_}2016{\_}Enzyme oscillation can enhance the thermodynamic efficiency of cellular metabolism consequence of anti-phase coupli.pdf:pdf},
issn = {1478-3975},
journal = {Physical Biology},
keywords = {82.39.Rt,87.17.Aa,87.17.Ee,88.05.Bc,article is available online,cellular metabolism,glycolytic oscillation,non-equilibrium thermodynamics,supplementary material for this,thermodynamic ef fi ciency,thermodynamic efficiency,yeast metabolic cycle},
number = {2},
pages = {026002},
publisher = {IOP Publishing},
title = {{Enzyme oscillation can enhance the thermodynamic efficiency of cellular metabolism: consequence of anti-phase coupling between reaction flux and affinity}},
url = {http://arxiv.org/abs/1511.03800{\%}5Cnhttp://stacks.iop.org/1478-3975/13/i=2/a=026002?key=crossref.bd748195ddecac8b7305bf97dbff6b26},
volume = {13},
