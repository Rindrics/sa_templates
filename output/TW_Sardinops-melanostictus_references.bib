author = {Sponaugle, S and Boulay, J N and Rankin, T L},
doi = {Doi 10.3354/Ab00370},
issn = {1864-7790},
journal = {Aquatic Biology},
keywords = {larval growth straits of florida growth-mortality},
language = {English},
pages = {263--273},
title = {{Growth- and size-selective mortality in pelagic larvae of a common reef fish}},
volume = {13},
year = {2011}
