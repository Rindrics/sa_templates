journal = {Fisheries Oceanography},
keywords = {Anchovy,Bio-physical coupling,Hydrodynamics,Recruitment,Regression analysis},
number = {2},
pages = {151--163},
title = {{The influence of mesoscale ocean processes on anchovy (Engraulis encrasicolus) recruitment in the Bay of Biscay estimated with a three-dimensional hydrodynamic mode}},
volume = {10},
year = {2001}
}
