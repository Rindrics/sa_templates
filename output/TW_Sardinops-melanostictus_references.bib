file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Zhang et al.{\_}2002{\_}Spatial distribution of ciliates, copepod nauplii and eggs, Engraulis japonicus post-larvae and microzooplankton herbi.pdf:pdf},
journal = {Aquatic Microbial Ecology},
number = {3},
pages = {249--259},
title = {{Spatial distribution of ciliates, copepod nauplii and eggs, Engraulis japonicus post-larvae and microzooplankton herbivorous activity in the Yellow Sea, China}},
