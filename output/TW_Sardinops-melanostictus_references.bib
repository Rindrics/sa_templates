author = {Schirripa, M J and Trexler, J C},
doi = {Doi 10.1016/S0165-7836(00)00135-1},
issn = {0165-7836},
journal = {Fisheries Research},
keywords = {back-calculation gear selectivity otoliths somatic},
language = {English},
pages = {83--89},
title = {{Effects of mortality and gear selectivity on the fish otolith radius-total length relation}},
volume = {46},
