author = {Bolle, L J and Dickey-Collas, M and van Beek, J K L and Erftemeijer, P L A and Witte, J I J and van der Veer, H W and Rijinsdorp, A D},
doi = {Doi 10.3354/Meps08177},
issn = {0171-8630},
journal = {Marine Ecology Progress Series},
keywords = {biophysical model hydrodynamic transport larval be},
language = {English},
pages = {195--211},
title = {{Variability in transport of fish eggs and larvae. III. Effects of hydrodynamics and larval behaviour on recruitment in plaice}},
volume = {390},
year = {2009}
}
