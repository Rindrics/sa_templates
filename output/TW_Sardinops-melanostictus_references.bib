issn = {0171-8630},
journal = {Marine Ecology Progress Series},
number = {Veer 1986},
pages = {105--116},
title = {{Growth, otolith growth and RNA/DNA ratios of larval plaice Pleuronectes platessa in the North Sea 1987 to 1989 }},
