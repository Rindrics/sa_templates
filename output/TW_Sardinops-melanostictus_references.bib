keywords = {Animals,Cell Membrane,Cell Membrane: metabolism,Dictyostelium,Dictyostelium: cytology,Dictyostelium: physiology,Enzyme Activation,Enzyme Activation: physiology,Models, Biological,Phosphatidylinositol 3-Kinases,Phosphatidylinositol 3-Kinases: metabolism,Protozoan Proteins,Protozoan Proteins: metabolism},
