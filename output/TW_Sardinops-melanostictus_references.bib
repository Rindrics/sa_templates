author = {Cury, P and Bakun, A and Crawford, R J M and Jarre, A and Quinones, R A and Shannon, L J and Verheye, H M C N - 000},
doi = {DOI 10.1006/jmsc.2000.0712},
file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Cury et al.{\_}2000{\_}Small pelagics in upwelling systems patterns of interaction and structural changes in wasp-waist ecosystems.pdf:pdf},
issn = {1054-3139},
