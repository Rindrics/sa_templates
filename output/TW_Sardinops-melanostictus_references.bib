issn = {15314332},
journal = {Comparative Biochemistry and Physiology -Part A : Molecular and Integrative Physiology},
keywords = {Circadian clock,Cry1,Cry2,Per1,Per2,Photoperiodism,Seasonality},
pages = {32--39},
pmid = {26093172},
publisher = {Elsevier Inc.},
title = {{Existence of a photoinducible phase for ovarian development and photoperiod-related alteration of clock gene expression in a damselfish}},
