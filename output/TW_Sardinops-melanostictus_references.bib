author = {Breitburg, D L and Rose, K A and Cowan, J H},
doi = {Doi 10.3354/Meps178039},
issn = {0171-8630},
journal = {Marine Ecology Progress Series},
keywords = {behavioral responses chesapeake bay fish predator},
language = {English},
pages = {39--54},
title = {{Linking water quality to larval survival: predation mortality of fish larvae in an oxygen-stratified water column}},
