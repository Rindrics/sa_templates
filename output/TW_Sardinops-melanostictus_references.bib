journal = {Progress in Oceanography},
keywords = {sprat sprattus-sprattus population connectivity so},
language = {English},
pages = {6--17},
title = {{Dispersal kernels and their drivers captured with a hydrodynamic model and spatial indices: A case study on anchovy (Engraulis encrasicolus) early life stages in the Bay of Biscay}},
