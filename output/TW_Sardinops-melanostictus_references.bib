doi = {10.1139/cjfas-56-6-925},
file = {:Users/00007738/Dropbox/AppDatas/Mendeley/Sibert et al.{\_}1999{\_}An advection-diffusion-reaction model for the estimation of fish movement parameters from tagging data, with applicat.pdf:pdf},
