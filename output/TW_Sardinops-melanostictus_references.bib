Cited References Count:63},
author = {Murase, H and Kawabata, A and Kubota, H and Nakagami, M and Amakasu, K and Abe, K and Miyashita, K and Oozeki, Y C N - 000},
doi = {DOI 10.1007/s12562-012-0508-2},
issn = {0919-9268},
journal = {Fisheries Science},
keywords = {abundance acoustic distribution hydroacoustic kuro},
language = {English},
pages = {761--773},
title = {{Basin-scale distribution pattern and biomass estimation of Japanese anchovy Engraulis japonicus in the western North Pacific}},
